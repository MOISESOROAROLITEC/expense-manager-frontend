  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3"
  },
  "peerDependencies": {
    "@apollo/react-components": "*",
    "@mui/base": "*",
    "apollo-cache": "*"
  }
