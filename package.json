{
  "name": "expense-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphql:download": "rover graph introspect http://localhost:3000/graphql --output src/app/graphql/schema/graphql-types.graphql --watch",
    "graphql:generate": "ts-node src/app/graphql/config/generate-types.ts",
    "schema:download": "npx apollo service:download --endpoint=http://localhost:3000/graphql src/app/graphql/graphql-schema.json",
    "schema:generate": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.1",
    "@mui/lab": "^5.0.0-alpha.138",
    "@mui/material": "^5.14.3",
    "@mui/x-date-pickers": "^6.10.2",
    "@nestjs/graphql": "^12.0.8",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.38",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "apollo": "^2.34.0",
    "apollo-client": "^2.6.10",
    "apollo-upload-client": "^17.0.0",
    "apollo-utilities": "^1.3.4",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "graphql": "^16.7.1",
    "graphql-react": "^20.0.0",
    "react": "^18.2.0",
    "react-apollo": "^3.1.5",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.15.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "sass": "^1.63.6",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
